+static bool compareImages(const QImage& i1, const QImage& i2, int count)
+{
+    if (i1.size() != i2.size())
+        return false;
+    for (int x = 0; x < count; ++x) {
+        for (int y = 0; y < count; ++y) {
+            QPoint point(x * i1.width() / count, y * i1.height() / count);
+            if (i1.pixel(point) != i2.pixel(point))
+                return false;
+        }
+    }
+
+    return true;
+}
