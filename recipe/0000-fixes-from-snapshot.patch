+            spy.wait()
+            compare(spy.count, 1)
+        }
+
+        function test_expectedLength() {
+            compare(spy.count, 0)
+            webView.url = Qt.resolvedUrl("../common/download.zip")
+            spy.wait()
+            compare(spy.count, 1)
+            compare(expectedLength, 325)
+        }
+
+        function test_succeeded() {
+            compare(spy.count, 0)
+            webView.url = Qt.resolvedUrl("../common/download.zip")
+            spy.wait()
+            compare(spy.count, 1)
+            downloadFinishedSpy.wait()
+            compare(totalBytes, expectedLength)
+        }
+    }
+}
